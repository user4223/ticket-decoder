  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "analyzer - debug",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-analyzer",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "analyzer - release",
      "program": "${workspaceFolder}/build/Release/bin/ticket-analyzer",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build release"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "decoder - debug - image (Muster 918-9 Schleswig-Holstein Ticket)",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-decoder",
      "args": [
        "-i",
        "images/Muster-Tickets nach UIC 918-9/Muster 918-9 Schleswig-Holstein Ticket.png"
      ],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "decoder - debug - raw (Muster 918-9 Schleswig-Holstein Ticket)",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-decoder",
      "args": [
        "-r",
        "out/Muster-Tickets nach UIC 918-9/Muster 918-9 Schleswig-Holstein Ticket.png_0.raw"
      ],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "decoder - debug - base64 (Muster 918-9 Schleswig-Holstein Ticket)",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-decoder",
      "args": [
        "-b",
        ""
      ],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "decoder - debug - base64 (Muster 918-3 City-Mobil Ticket)",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-decoder",
      "args": [
        "-b",
        ""
      ],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "tests - debug",
      "program": "${workspaceFolder}/build/Debug/bin/ticket-decoder-test",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build debug"
    }
  ]
}